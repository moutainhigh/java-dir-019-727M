public class BigNumber {
    public static void main(String[] args) {
        int bigNum = 99999999;
        long number = 9999999999999999L;

        System.out.println(1/3.0);

        System.out.println(1<3);
        System.out.println(1>3);

        System.out.println("ABC");
        System.out.println('A');
    }
}
