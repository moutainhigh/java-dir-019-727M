	public static void main(String args[]) {
	Date date=new Date();
	
	SimpleDateFormat df=new SimpleDateFormat("yyyy-MM-dd hh:mm:ss");
	String time=df.format(date);
	System.out.println(time);
	
	Integer a=2;
	Integer b=1;
	System.out.println(a.toString().compareTo(b.toString()+6));

	}
}